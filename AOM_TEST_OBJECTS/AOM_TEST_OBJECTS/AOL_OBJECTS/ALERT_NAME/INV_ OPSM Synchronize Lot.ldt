BEGIN ALR_ALERTS "INV" "INV: OPSM Synchronize Lot"
  OWNER = "ORACLE12.2.10"
  ALERT_CONDITION_TYPE = "E"
  ENABLED_FLAG = "Y"
  START_DATE_ACTIVE = "2011/01/24 00:00:00"
  TABLE_APPLICATION_SHORT_NAME = "INV"
  FREQUENCY_TYPE = "O"
  INSERT_FLAG = "Y"
  UPDATE_FLAG = "Y"
  MAINTAIN_HISTORY_DAYS = "0"
  SQL_STATEMENT_TEXT =
 "SELECT\n\
             msibk.inventory_item_id\n\
            ,replace(msibk.concatenated_segments,CHR(38),'^^^^')\n\
            ,replace(msibk.description,CHR(38),'^^^^')\n\
            ,msibk.primary_uom_code\n\
            ,msibk.lot_control_code\n\
            ,msibk.serial_number_control_code\n\
            ,1\n\
            ,msibk.organization_id\n\
            ,mp.organization_code\n\
            ,mln.age\n\
            ,to_char(mln.best_by_date,'DD-MM-YYYY HH24:MI:SS')\n\
            ,to_char(mln.change_date,'DD-MM-YYYY HH24:MI:SS')  \n\
            ,to_char(mln.expiration_date,'DD-MM-YYYY HH24:MI:SS')\n\
            ,replace(mln.grade_code,CHR(38),'^^^^')\n\
            ,mln.length\n\
            ,mln.length_uom\n\
            ,replace(mln.description,CHR(38),'^^^^')\n\
            ,replace(mln.lot_number,CHR(38),'^^^^')\n\
            ,to_char(mln.maturity_date,'DD-MM-YYYY HH24:MI:SS')\n\
            ,to_char(mln.origination_date,'DD-MM-YYYY HH24:MI:SS')\n\
            ,replace(mln.place_of_origin,CHR(38),'^^^^')\n\
            ,mln.recycled_content\n\
            ,to_char(mln.retest_date,'DD-MM-YYYY HH24:MI:SS')\n\
            ,mln.thickness\n\
            ,mln.thickness_uom\n\
            ,mln.volume\n\
            ,mln.volume_uom\n\
            ,mln.width\n\
            ,mln.width_uom\n\
            ,replace(mln.expiration_action_code,CHR(38),'^^^^')\n\
            ,to_char(mln.expiration_action_date,'DD-MM-YYYY HH24:MI:SS')\n\
            ,replace(mln.parent_lot_number,CHR(38),'^^^^')\n\
            ,mln.reservable_type\n\
            ,mln.origination_type\n\
            ,mln.gen_object_id\n\
INTO \n\
             &inventory_item_id\n\
            ,&concatenated_segments\n\
            ,&item_description\n\
            ,&primary_uom_code\n\
            ,&lot_control_code\n\
            ,&serial_number_control_code\n\
            ,&cross_reference\n\
            ,&organization_id\n\
            ,&organization_code\n\
            ,&age\n\
            ,&best_by_date\n\
            ,&change_date\n\
            ,&expiration_date\n\
            ,&grade_code\n\
            ,&length\n\
            ,&length_uom\n\
            ,&lot_description\n\
            ,&lot_number\n\
            ,&maturity_date\n\
            ,&origination_date\n\
            ,&place_of_origin\n\
            ,&recycled_content\n\
            ,&retest_date\n\
            ,&thickness\n\
            ,&thickness_uom\n\
            ,&volume\n\
            ,&volume_uom\n\
            ,&width\n\
            ,&width_uom\n\
            ,&expiration_action_code\n\
            ,&expiration_action_date\n\
            ,&parent_lot_number\n\
            ,&reservable_type\n\
            ,&origination_type\n\
            ,&gen_object_id\n\
FROM         mtl_lot_numbers mln\n\
            ,mtl_system_items_b_kfv msibk\n\
            ,mtl_cross_references_vl mcr\n\
            ,mtl_parameters mp\n\
       WHERE     msibk.inventory_item_id           =   mcr.inventory_item_id\n\
         AND     mln.inventory_item_id             =   msibk.inventory_item_id\n\
         AND     mln.organization_id               =   msibk.organization_id\n\
         AND     mp.organization_id                =   msibk.organization_id\n\
         AND     mcr.cross_reference_type          =   'OPSM INTEGRATED'\n\
         AND     mcr.cross_reference               =   'YES'\n\
         AND     mp.master_organization_id         =   mcr.organization_id\n\
         AND     mp.opsm_enabled_FLAG              =   'Y'\n\
         AND     msibk.lot_control_code            =   2\n\
         AND     NOT EXISTS\n\
                            (SELECT 'Y'\n\
                               FROM MTL_CROSS_REFERENCES_VL\n\
                              WHERE CROSS_REFERENCE_TYPE      = 'OPSM INTEGRATED'\n\
                                AND ORGANIZATION_ID           = msibk.organization_id\n\
                                AND INVENTORY_ITEM_ID         = msibk.inventory_item_id\n\
                                AND UPPER(CROSS_REFERENCE)    = 'NO')\n\
        AND      mln.ROWID = :ROWID;"
  TABLE_NAME = "MTL_LOT_NUMBERS"
  LAST_UPDATE_DATE = "2011/01/24"


